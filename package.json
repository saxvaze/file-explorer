{
  "name": "file-explorer",
  "version": "0.1.0",
  "private": true,
  "main": "server/index.js",
  "dependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.14",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run start-react\" \"npm run start-electron\"",
    "start-electron": "tsc -w ./server/index.ts",
    "start-react": "react-app-rewired start",
    "build-react": "react-app-rewired build",
    "launch-electron": "electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.5.0",
    "electron": "^16.0.5",
    "electron-is-dev": "^2.0.0",
    "react-app-rewired": "^2.1.8",
    "wait-on": "^6.0.0"
  }
